{
  "name": "@dreamer/plugins",
  "version": "1.0.0-beta.3",
  "description": "dweb 框架的官方插件集合",
  "exports": {
    ".": "./src/mod.ts",
    "./tailwindcss": "./src/tailwindcss/mod.ts",
    "./unocss": "./src/unocss/mod.ts",
    "./i18n": "./src/i18n/mod.ts",
    "./i18n/client": "./src/i18n/client/mod.ts",
    "./seo": "./src/seo/mod.ts",
    "./pwa": "./src/pwa/mod.ts",
    "./analytics": "./src/analytics/mod.ts",
    "./theme": "./src/theme/mod.ts",
    "./theme/client": "./src/theme/client/mod.ts",
    "./compression": "./src/compression/mod.ts",
    "./cors": "./src/cors/mod.ts",
    "./ratelimit": "./src/ratelimit/mod.ts",
    "./security": "./src/security/mod.ts",
    "./auth": "./src/auth/mod.ts",
    "./static": "./src/static/mod.ts",
    "./upload": "./src/upload/mod.ts",
    "./captcha": "./src/captcha/mod.ts",
    "./markdown": "./src/markdown/mod.ts",
    "./image": "./src/image/mod.ts",
    "./payment": "./src/payment/mod.ts",
    "./social": "./src/social/mod.ts",
    "./notification": "./src/notification/mod.ts"
  },
  "publish": {
    "exclude": [
      "tests/",
      ".github/",
      "*.test.ts",
      "TEST_REPORT.md"
    ]
  },
  "imports": {
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.0-beta.22",
    "@dreamer/plugin": "jsr:@dreamer/plugin@^1.0.0-beta.3",
    "@dreamer/service": "jsr:@dreamer/service@^1.0.0-beta.4",
    "@dreamer/config": "jsr:@dreamer/config@^1.0.0-beta.3",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.0-beta.2",
    "@dreamer/server": "jsr:@dreamer/server@^1.0.0-beta.1",
    "@dreamer/image": "jsr:@dreamer/image@^1.0.0-beta.3",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0-beta.39"
  },
  "tasks": {
    "test": "deno test --allow-all tests/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
